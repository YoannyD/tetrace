<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="cron_return_out_date_assignation" model="ir.cron">
            <field name="name">Devolver asignaciones</field>
            <field name="model_id" ref="model_stock_product_assignation"/>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>        <!-- To decide clearly -->
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(datetime.now() + timedelta(1)).strftime('%Y-%m-%d 23:00:00')"/>
            <field name="numbercall">-1</field>
            <field name="priority">1</field>
            <field name="state">code</field>
            <field name="code">model.cron_return_out_date_assignation()</field>
        </record>
    </data>
</odoo>
