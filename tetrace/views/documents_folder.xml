<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="folder_view_form" model="ir.ui.view">
        <field name="name">Workspace form tetrace</field>
        <field name="model">documents.folder</field>
        <field name="inherit_id" ref="documents.folder_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after">
                <field name="view_employee"/>
                <field name="view_all"/>
            </xpath>
        </field>
    </record>
</odoo>
