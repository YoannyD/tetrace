<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="mis_report_view_form">
        <field name="name">mis.report.view.form.tetrace</field>
        <field name="inherit_id" ref="mis_builder.mis_report_view_form"/>
        <field name="model">mis.report</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_lines_source']" position="after">
                <field name="mostrar_cuenta_consolidacion"/>
            </xpath>
        </field>
    </record>
</odoo>
