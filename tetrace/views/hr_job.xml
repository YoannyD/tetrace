<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="view_hr_job_form">
        <field name="name">hr.job.form.tetrace</field>
        <field name="inherit_id" ref="hr.view_hr_job_form"/>
        <field name="model">hr.job</field>
        <field name="arch" type="xml">
            <xpath expr="//sheet[1]" position="inside">
                <group string="Necesidades">
                    <field name="proyecto_necesidad_ids" nolabel="1">
                        <tree editable="bottom">
                            <field name="project_id"/>
                            <field name="nacionalidad"/>
                            <field name="maquina"/>
                            <field name="formacion" widget="many2many_tags"/>
                            <field name="fecha_entrega"/>
                            <field name="salario"/>
                            <field name="necesidad"/>
                            <field name="realizado"/>   
                        </tree>
                    </field>
                </group>
            </xpath>
        </field>
    </record>
</odoo>