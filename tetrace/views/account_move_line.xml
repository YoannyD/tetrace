<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="view_move_line_form">
        <field name="name">account.move.line.form.tetrace</field>
        <field name="inherit_id" ref="account.view_move_line_form" />
        <field name="model">account.move.line</field>
        <field name="arch" type="xml">
            <xpath expr="//notebook[1]/page[1]/group[1]" position="inside">
                <group name="informe" string="Informe">
                    <field name="categoria_ids" widget="many2many"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
