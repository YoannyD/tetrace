<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="tetrace_importar_tickelia_form">
        <field name="name">tetrace.importar_tickelia</field>
        <field name="model">tetrace.importar_tickelia</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="file" filename="name"/>
                </group>
                <footer>
                    <button name="action_import_from_excel" string="Importar" type="object" class="btn-primary"/>
                    <button string="_Cancel" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
