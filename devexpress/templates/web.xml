<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" inherit_id="web.assets_frontend" name="Portal Assets">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/devexpress/static/src/css/dx.common.css" />
            <link rel="stylesheet" type="text/css" href="/devexpress/static/src/css/dx.light.css" />
            <link rel="stylesheet" type="text/css" href="/devexpress/static/src/css/dx.light.compact.css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script src="/devexpress/static/src/js/cldr.min.js"/>
            <script src="/devexpress/static/src/js/cldr/event.min.js"/>
            <script src="/devexpress/static/src/js/cldr/supplemental.min.js"/>
            <script src="/devexpress/static/src/js/cldr/unresolved.min.js"/>
            <script src="/devexpress/static/src/js/globalize.min.js"/>
            <script src="/devexpress/static/src/js/globalize/message.min.js"/>
            <script src="/devexpress/static/src/js/globalize/number.min.js"/>
            <script src="/devexpress/static/src/js/globalize/currency.min.js"/>
            <script src="/devexpress/static/src/js/globalize/date.min.js"/>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.6/quill.min.js"/>
            <script type="text/javascript" src="/devexpress/static/src/js/dx.all.js"/>
            <script type="text/javascript" src="/devexpress/static/src/js/dx.messages.es.js"/>
            <script type="text/javascript" src="/devexpress/static/src/js/localization/dx.messages.es.js"/>

            <script src="https://unpkg.com/devextreme-cldr-data/es.js"/>
            <script src="https://unpkg.com/devextreme-aspnet-data@2.0.0/js/dx.aspnet.data.js"/>
        </xpath>
    </template>
</odoo>
